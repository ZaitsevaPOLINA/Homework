#include <vector>
#include <iostream>
#include <string>

class vector
{
    int*Array;
    int size;
    int cap;
    public:
    vector( int new_size)
    {
    for(int =0; i< cap; i++)
    {
    this->Array=Array[i];
    new_size++
    }
    *Array[i+1]=NULL;
    }
    ~vector()
    {
		if(this->Array != NULL)
		{
			delete(this->Array, this->size);
		}		
		this->size = 0;
		this->Array = new a[this->size];
	}	
    void push_back( int a)
    {
    
		a*_result = new a[++this->size];

		for(int i= 0; i < this->size; i++)
		{
			if(i!= this->size-1)
			{
				_result[i] = this->Array[i];
			}
			else
			{
				_result[i] = a;
				break;
			}
		}
	}
;
    void append(int new_size, int n)
    {
    new_size=size*n;
    for(i=size; i< new_size-size; i++)
    {
    Array[i]-> Array[i+1]=NULL;
    }
    
    }
    const int & operator[](int i) const{}
    {
        return Array[i];
        }
