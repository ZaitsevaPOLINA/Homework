// Example program
#include <iostream>
#include <string>

int main(){

	int n,m;

	cin >> n,m;

	 int**Matrix = new int*[n+1];
	for (int i = 0; i <=n; i++)
		for (int j = 0; j <=m; j++){
			Matrix[n][m] = 0;
			Matrix[m][n] = 0;

	for (int i = 0; i <= m; i++)
		
    int N, M;
 		cin >> N >> M;
			Matrix[N][M] = 1;
			Matrix[M][N] = 1;
		}


        vector<vector<int> > gr(n); 
      for (int i=0; i<m; i++) {
          int N1,N2;
        cin >> N1 >> N2; 
         gr[N1].push_back(N2); 
         }



      int**Matrix1 = new int*[m+1];
	for (int i = 0; i <=n; i++)
		for (int j = 0; j <=m; j++){
			Matrix[n][m] = 0;
			Matrix[m][n] = 0;
		}
     for (int i = 0; i < m; i++) {
 		int N, M;
 		cin >> N >> M;
 		 Matrix[M][N]= 1;
         Matrix[N][M]= 1;
 	}

}
